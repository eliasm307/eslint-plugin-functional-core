{
  "name": "eslint-plugin-functional-core",
  "version": "0.0.6",
  "description": "TBC",
  "license": "MIT",
  "author": "Elias Mangoro",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "index.d.ts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "dist",
    "docs",
    "index.d.ts",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run build",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint . --fix",
    "update:eslint-docs": "npm run build && eslint-doc-generator",
    "test": "echo 'Testing...' && jest --coverage",
    "testw": "echo 'Testing in watch mode...' && jest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"./**/*.{ts,mts,cts,tsx,js,mjs,cjs,jsx,json,md,css}\" --ignore-path ../../.prettierignore",
    "beforepublish": "npm run lint:js && npm run test && npm run typecheck && npm run format && npm run build",
    "publish:patch": "ts-node scripts/publish.ts patch",
    "publish:minor": "ts-node scripts/publish.ts minor",
    "publish:major": "ts-node scripts/publish.ts major"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.59.9"
  },
  "devDependencies": {
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": ">=7"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}